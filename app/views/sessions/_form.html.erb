<%= form_for(:session, :html => {class: "form-horizontal", role: "form"}, url: sessions_path) do |form| %>

<div class="container">
  <form class="form-horizontal">
    <div class="form-group">
      <%= form.label :email, class: "col-sm-4 control-label" %>
      <div class="col-sm-4">
        <%= form.email_field :email, class: "form-control", id: "inputEmail3", placeholder: "Email" %>
      </div>
    </div>

     <div class="form-group">
       <%= form.label :password, class: "col-sm-4 control-label" %>
       <div class="col-sm-4">
         <%= form.password_field :email, class: "form-control", id: "inputPassword3", placeholder: "Password" %>
      </div>
    </div>

    <div class="form-group">
      <div class="col-sm-offset-4 col-sm-8">
        <div class="checkbox">
          <label>
            <input type="checkbox"> Remember me
          </label>
        </div>
      </div>
    </div>

    <div class="form-group">
      <div class="col-sm-offset-4 col-sm-10">
        <%= form.submit "Sign In", class: "btn btn-default" %>
      </div>
    </div>
  </form>
</div>

  <% end %>
  
